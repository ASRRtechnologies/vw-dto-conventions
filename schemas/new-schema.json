{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "projectName",
      "type",
      "source",
      "configuration"
    ],
    "properties": {
      "projectName": {
        "$id": "#/properties/projectName",
        "type": "string",
        "title": "The Projectname Schema",
        "default": "",
        "examples": [
          "ProjectName"
        ],
        "pattern": "^(.*)$"
      },
      "type": {
        "$id": "#/properties/type",
        "type": "string",
        "title": "The Type Schema",
        "default": "",
        "examples": [
          "project"
        ],
        "pattern": "^(.*)$"
      },
      "source": {
        "$id": "#/properties/source",
        "type": "string",
        "title": "The Source Schema",
        "default": "",
        "examples": [
          "NameOfSource(You)"
        ],
        "pattern": "^(.*)$"
      },
      "configuration": {
        "$id": "#/properties/configuration",
        "type": "object",
        "title": "The Configuration Schema",
        "required": [
          "name",
          "logo",
          "contact",
          "blocks"
        ],
        "properties": {
          "name": {
            "$id": "#/properties/configuration/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "ProjectName"
            ],
            "pattern": "^(.*)$"
          },
          "logo": {
            "$id": "#/properties/configuration/properties/logo",
            "type": "string",
            "title": "The Logo Schema",
            "default": "",
            "examples": [
              "boele_en_van_eesteren"
            ],
            "pattern": "^(.*)$"
          },
          "contact": {
            "$id": "#/properties/configuration/properties/contact",
            "type": "string",
            "title": "The Contact Schema",
            "default": "",
            "examples": [
              "Meneer Man"
            ],
            "pattern": "^(.*)$"
          },
          "blocks": {
            "$id": "#/properties/configuration/properties/blocks",
            "type": "array",
            "title": "The Blocks Schema",
            "items": {
              "$id": "#/properties/configuration/properties/blocks/items",
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "name",
                "position",
                "rotation",
                "houses"
              ],
              "properties": {
                "name": {
                  "$id": "#/properties/configuration/properties/blocks/items/properties/name",
                  "type": "string",
                  "title": "The Name Schema",
                  "default": "",
                  "examples": [
                    "BlockName1"
                  ],
                  "pattern": "^(.*)$"
                },
                "position": {
                  "$id": "#/properties/configuration/properties/blocks/items/properties/position",
                  "type": "object",
                  "title": "The Position Schema",
                  "required": [
                    "x",
                    "z",
                    "y"
                  ],
                  "properties": {
                    "x": {
                      "$id": "#/properties/configuration/properties/blocks/items/properties/position/properties/x",
                      "type": "integer",
                      "title": "The X Schema",
                      "default": 0,
                      "examples": [
                        4498
                      ]
                    },
                    "z": {
                      "$id": "#/properties/configuration/properties/blocks/items/properties/position/properties/z",
                      "type": "integer",
                      "title": "The Z Schema",
                      "default": 0,
                      "examples": [
                        0
                      ]
                    },
                    "y": {
                      "$id": "#/properties/configuration/properties/blocks/items/properties/position/properties/y",
                      "type": "integer",
                      "title": "The Y Schema",
                      "default": 0,
                      "examples": [
                        16729
                      ]
                    }
                  }
                },
                "rotation": {
                  "$id": "#/properties/configuration/properties/blocks/items/properties/rotation",
                  "type": "object",
                  "title": "The Rotation Schema",
                  "required": [
                    "x",
                    "y",
                    "z"
                  ],
                  "properties": {
                    "x": {
                      "$id": "#/properties/configuration/properties/blocks/items/properties/rotation/properties/x",
                      "type": "integer",
                      "title": "The X Schema",
                      "default": 0,
                      "examples": [
                        1
                      ]
                    },
                    "y": {
                      "$id": "#/properties/configuration/properties/blocks/items/properties/rotation/properties/y",
                      "type": "integer",
                      "title": "The Y Schema",
                      "default": 0,
                      "examples": [
                        0
                      ]
                    },
                    "z": {
                      "$id": "#/properties/configuration/properties/blocks/items/properties/rotation/properties/z",
                      "type": "integer",
                      "title": "The Z Schema",
                      "default": 0,
                      "examples": [
                        0
                      ]
                    }
                  }
                },
                "houses": {
                  "$id": "#/properties/configuration/properties/blocks/items/properties/houses",
                  "type": "array",
                  "title": "The Houses Schema",
                  "items": {
                    "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items",
                    "type": "object",
                    "title": "The Items Schema",
                    "required": [
                      "name",
                      "houseNumber",
                      "type",
                      "configOptions",
                      "position",
                      "rotation"
                    ],
                    "properties": {
                      "name": {
                        "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/name",
                        "type": "string",
                        "title": "The Name Schema",
                        "default": "",
                        "examples": [
                          "HouseName"
                        ],
                        "pattern": "^(.*)$"
                      },
                      "houseNumber": {
                        "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/houseNumber",
                        "type": "integer",
                        "title": "The Housenumber Schema",
                        "default": 0,
                        "examples": [
                          1
                        ]
                      },
                      "type": {
                        "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/type",
                        "type": "string",
                        "title": "The Type Schema",
                        "default": "",
                        "examples": [
                          "TypeB"
                        ],
                        "pattern": "^(.*)$"
                      },
                      "configOptions": {
                        "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/configOptions",
                        "type": "object",
                        "title": "The Configoptions Schema",
                        "required": [
                          "plaatsing",
                          "beukbreedte",
                          "daktype"
                        ],
                        "properties": {
                          "plaatsing": {
                            "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/configOptions/properties/plaatsing",
                            "type": "string",
                            "title": "The Plaatsing Schema",
                            "default": "",
                            "examples": [
                              "links"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "beukbreedte": {
                            "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/configOptions/properties/beukbreedte",
                            "type": "string",
                            "title": "The Beukbreedte Schema",
                            "default": "",
                            "examples": [
                              "5400"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "daktype": {
                            "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/configOptions/properties/daktype",
                            "type": "string",
                            "title": "The Daktype Schema",
                            "default": "",
                            "examples": [
                              "langskap"
                            ],
                            "pattern": "^(.*)$"
                          }
                        }
                      },
                      "position": {
                        "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/position",
                        "type": "object",
                        "title": "The Position Schema",
                        "required": [
                          "x",
                          "z",
                          "y"
                        ],
                        "properties": {
                          "x": {
                            "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/position/properties/x",
                            "type": "integer",
                            "title": "The X Schema",
                            "default": 0,
                            "examples": [
                              -280
                            ]
                          },
                          "z": {
                            "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/position/properties/z",
                            "type": "integer",
                            "title": "The Z Schema",
                            "default": 0,
                            "examples": [
                              0
                            ]
                          },
                          "y": {
                            "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/position/properties/y",
                            "type": "integer",
                            "title": "The Y Schema",
                            "default": 0,
                            "examples": [
                              0
                            ]
                          }
                        }
                      },
                      "rotation": {
                        "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/rotation",
                        "type": "object",
                        "title": "The Rotation Schema",
                        "required": [
                          "x",
                          "y",
                          "z"
                        ],
                        "properties": {
                          "x": {
                            "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/rotation/properties/x",
                            "type": "integer",
                            "title": "The X Schema",
                            "default": 0,
                            "examples": [
                              1
                            ]
                          },
                          "y": {
                            "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/rotation/properties/y",
                            "type": "integer",
                            "title": "The Y Schema",
                            "default": 0,
                            "examples": [
                              0
                            ]
                          },
                          "z": {
                            "$id": "#/properties/configuration/properties/blocks/items/properties/houses/items/properties/rotation/properties/z",
                            "type": "integer",
                            "title": "The Z Schema",
                            "default": 0,
                            "examples": [
                              0
                            ]
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }