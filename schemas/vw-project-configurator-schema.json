{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://form.nl/conventions.json",
  "type": "object",
  "title": "FORM json file structure conventions",
  "required": [
    "configuration_type",
    "content"
  ],
  "properties": {
    "configuration_type": {
      "$id": "#/properties/configuration_type",
      "type": "string",
      "title": "The Configuration_type Schema",
      "default": "project",
      "examples": [
        "project",
        "facade"
      ],
      "pattern": "project|facade"
    },
    "content": {
      "$id": "#/properties/content",
      "type": "object",
      "title": "The Content Schema",
      "required": [
        "name",
        "logo",
        "blocks"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/content/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "Coolname"
          ],
          "pattern": "^(.*)$"
        },
        "logo": {
          "$id": "#/properties/content/properties/logo",
          "type": "string",
          "title": "The Logo Schema",
          "default": "",
          "examples": [
            "testlogo"
          ],
          "pattern": "^(.*)$"
        },
        "blocks": {
          "$id": "#/properties/content/properties/blocks",
          "type": "array",
          "title": "The Blocks Schema",
          "items": {
            "$id": "#/properties/content/properties/blocks/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "name",
              "position",
              "rotation",
              "houses"
            ],
            "properties": {
              "name": {
                "$id": "#/properties/content/properties/blocks/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "Woningblok#2"
                ],
                "pattern": "^(.*)$"
              },
              "position": {
                "$id": "#/properties/content/properties/blocks/items/properties/position",
                "type": "object",
                "title": "The Position Schema",
                "required": [
                  "x",
                  "y",
                  "z"
                ],
                "properties": {
                  "x": {
                    "$id": "#/properties/content/properties/blocks/items/properties/position/properties/x",
                    "type": "integer",
                    "title": "The X Schema",
                    "default": 0,
                    "examples": [
                      0
                    ]
                  },
                  "y": {
                    "$id": "#/properties/content/properties/blocks/items/properties/position/properties/y",
                    "type": "integer",
                    "title": "The Y Schema",
                    "default": 0,
                    "examples": [
                      0
                    ]
                  },
                  "z": {
                    "$id": "#/properties/content/properties/blocks/items/properties/position/properties/z",
                    "type": "integer",
                    "title": "The Z Schema",
                    "default": 0,
                    "examples": [
                      0
                    ]
                  }
                }
              },
              "rotation": {
                "$id": "#/properties/content/properties/blocks/items/properties/rotation",
                "type": "object",
                "title": "The Rotation Schema",
                "required": [
                  "x",
                  "y",
                  "z"
                ],
                "properties": {
                  "x": {
                    "$id": "#/properties/content/properties/blocks/items/properties/rotation/properties/x",
                    "type": "integer",
                    "title": "The X Schema",
                    "default": 0,
                    "examples": [
                      1
                    ]
                  },
                  "y": {
                    "$id": "#/properties/content/properties/blocks/items/properties/rotation/properties/y",
                    "type": "integer",
                    "title": "The Y Schema",
                    "default": 0,
                    "examples": [
                      0
                    ]
                  },
                  "z": {
                    "$id": "#/properties/content/properties/blocks/items/properties/rotation/properties/z",
                    "type": "integer",
                    "title": "The Z Schema",
                    "default": 0,
                    "examples": [
                      0
                    ]
                  }
                }
              },
              "houses": {
                "$id": "#/properties/content/properties/blocks/items/properties/houses",
                "type": "array",
                "title": "The Houses Schema",
                "items": {
                  "$id": "#/properties/content/properties/blocks/items/properties/houses/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "required": [
                    "name",
                    "house_number",
                    "position",
                    "rotation",
                    "elements"
                  ],
                  "properties": {
                    "name": {
                      "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/name",
                      "type": "string",
                      "title": "The Name Schema",
                      "default": "",
                      "examples": [
                        "Zadeldak_links"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "house_number": {
                      "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/house_number",
                      "type": "integer",
                      "title": "The House_number Schema",
                      "default": 0,
                      "examples": [
                        1
                      ]
                    },
                    "position": {
                      "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/position",
                      "type": "object",
                      "title": "The Position Schema",
                      "required": [
                        "x",
                        "y",
                        "z"
                      ],
                      "properties": {
                        "x": {
                          "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/position/properties/x",
                          "type": "integer",
                          "title": "The X Schema",
                          "default": 0,
                          "examples": [
                            280
                          ]
                        },
                        "y": {
                          "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/position/properties/y",
                          "type": "integer",
                          "title": "The Y Schema",
                          "default": 0,
                          "examples": [
                            0
                          ]
                        },
                        "z": {
                          "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/position/properties/z",
                          "type": "integer",
                          "title": "The Z Schema",
                          "default": 0,
                          "examples": [
                            0
                          ]
                        }
                      }
                    },
                    "rotation": {
                      "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/rotation",
                      "type": "object",
                      "title": "The Rotation Schema",
                      "required": [
                        "x",
                        "y",
                        "z"
                      ],
                      "properties": {
                        "x": {
                          "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/rotation/properties/x",
                          "type": "integer",
                          "title": "The X Schema",
                          "default": 0,
                          "examples": [
                            1
                          ]
                        },
                        "y": {
                          "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/rotation/properties/y",
                          "type": "integer",
                          "title": "The Y Schema",
                          "default": 0,
                          "examples": [
                            0
                          ]
                        },
                        "z": {
                          "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/rotation/properties/z",
                          "type": "integer",
                          "title": "The Z Schema",
                          "default": 0,
                          "examples": [
                            0
                          ]
                        }
                      }
                    },
                    "elements": {
                      "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements",
                      "type": "array",
                      "title": "The Elements Schema",
                      "items": {
                        "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items",
                        "type": "object",
                        "title": "The Items Schema",
                        "required": [
                          "name",
                          "id",
                          "level",
                          "position",
                          "rotation"
                        ],
                        "properties": {
                          "name": {
                            "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/name",
                            "type": "string",
                            "title": "The Name Schema",
                            "default": "",
                            "examples": [
                              "NAAM_OPGEGEVEN_DOOR_GEBRUIKER"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "id": {
                            "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/id",
                            "type": "string",
                            "title": "The Id Schema",
                            "default": "",
                            "examples": [
                              "voorgevel"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "level": {
                            "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/level",
                            "type": "integer",
                            "title": "The Level Schema",
                            "default": 0,
                            "examples": [
                              0
                            ]
                          },
                          "position": {
                            "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/position",
                            "type": "object",
                            "title": "The Position Schema",
                            "required": [
                              "x",
                              "y",
                              "z"
                            ],
                            "properties": {
                              "x": {
                                "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/position/properties/x",
                                "type": "integer",
                                "title": "The X Schema",
                                "default": 0,
                                "examples": [
                                  0
                                ]
                              },
                              "y": {
                                "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/position/properties/y",
                                "type": "integer",
                                "title": "The Y Schema",
                                "default": 0,
                                "examples": [
                                  0
                                ]
                              },
                              "z": {
                                "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/position/properties/z",
                                "type": "integer",
                                "title": "The Z Schema",
                                "default": 0,
                                "examples": [
                                  0
                                ]
                              }
                            }
                          },
                          "rotation": {
                            "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/rotation",
                            "type": "object",
                            "title": "The Rotation Schema",
                            "required": [
                              "x",
                              "y",
                              "z"
                            ],
                            "properties": {
                              "x": {
                                "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/rotation/properties/x",
                                "type": "integer",
                                "title": "The X Schema",
                                "default": 0,
                                "examples": [
                                  1
                                ]
                              },
                              "y": {
                                "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/rotation/properties/y",
                                "type": "integer",
                                "title": "The Y Schema",
                                "default": 0,
                                "examples": [
                                  0
                                ]
                              },
                              "z": {
                                "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/rotation/properties/z",
                                "type": "integer",
                                "title": "The Z Schema",
                                "default": 0,
                                "examples": [
                                  0
                                ]
                              }
                            }
                          },
                          "elements": {
                            "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/elements",
                            "type": "array",
                            "title": "The Elements Schema",
                            "items": {
                              "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/elements/items",
                              "type": "object",
                              "title": "The Items Schema",
                              "required": [
                                "type",
                                "position",
                                "width",
                                "height"
                              ],
                              "properties": {
                                "type": {
                                  "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/elements/items/properties/type",
                                  "type": "string",
                                  "title": "The Type Schema",
                                  "default": "",
                                  "examples": [
                                    "kozijn"
                                  ],
                                  "pattern": "^(.*)$"
                                },
                                "position": {
                                  "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/elements/items/properties/position",
                                  "type": "object",
                                  "title": "The Position Schema",
                                  "required": [
                                    "x",
                                    "y",
                                    "z"
                                  ],
                                  "properties": {
                                    "x": {
                                      "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/elements/items/properties/position/properties/x",
                                      "type": "integer",
                                      "title": "The X Schema",
                                      "default": 0,
                                      "examples": [
                                        400
                                      ]
                                    },
                                    "y": {
                                      "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/elements/items/properties/position/properties/y",
                                      "type": "integer",
                                      "title": "The Y Schema",
                                      "default": 0,
                                      "examples": [
                                        70
                                      ]
                                    },
                                    "z": {
                                      "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/elements/items/properties/position/properties/z",
                                      "type": "integer",
                                      "title": "The Z Schema",
                                      "default": 0,
                                      "examples": [
                                        300
                                      ]
                                    }
                                  }
                                },
                                "width": {
                                  "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/elements/items/properties/width",
                                  "type": "integer",
                                  "title": "The Width Schema",
                                  "default": 0,
                                  "examples": [
                                    700
                                  ]
                                },
                                "height": {
                                  "$id": "#/properties/content/properties/blocks/items/properties/houses/items/properties/elements/items/properties/elements/items/properties/height",
                                  "type": "integer",
                                  "title": "The Height Schema",
                                  "default": 0,
                                  "examples": [
                                    700
                                  ]
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}